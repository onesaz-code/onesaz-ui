{
  "name": "@onesaz/ui",
  "version": "0.2.1",
  "description": "Onesaz UI component library - React components with theming and Tailwind CSS",
  "type": "module",
  "license": "MIT",
  "author": "Onesaz <engineering@onesaz.com>",
  "homepage": "https://github.com/onesaz/onesaz-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onesaz/onesaz-ui.git",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/onesaz/onesaz-ui/issues"
  },
  "keywords": [
    "react",
    "ui-components",
    "design-system",
    "tailwindcss",
    "theming",
    "dark-mode",
    "onesaz"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@onesaz/tokens": "*",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.15",
    "tsup": "^8.3.5",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
