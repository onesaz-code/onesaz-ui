{
  "name": "@onesaz/ui",
  "version": "0.3.16",
  "description": "Onesaz UI component library - React components with theming and Tailwind CSS",
  "type": "module",
  "license": "MIT",
  "author": "Onesaz <engineering@onesaz.com>",
  "homepage": "https://github.com/onesaz/onesaz-ui#readme",
