{
  "name": "@onesaz/tokens",
  "version": "0.1.1",
  "description": "Design tokens for Onesaz UI component library - colors, spacing, typography, and theming",
  "type": "module",
  "license": "MIT",
  "author": "Onesaz <engineering@onesaz.com>",
  "homepage": "https://github.com/onesaz/onesaz-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onesaz/onesaz-ui.git",
    "directory": "packages/tokens"
  },
  "bugs": {
    "url": "https://github.com/onesaz/onesaz-ui/issues"
  },
  "keywords": [
    "design-tokens",
    "design-system",
    "theming",
    "colors",
    "onesaz"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
